# include <stdio.h>
int main(){
float a ;
printf(" Enter your first number here = " );
scanf("%f", &a);
float b ;
printf(" Enter your second number here = ");
scanf("%f",&b);
printf(" The sum of two number is = %f \n ", a + b);
printf(" The subtraction of two number is = %f \n ", a - b);
printf(" The multiplication of two number is = %f \n ", a  * b);
printf(" The divison of two number is = %f \n ", a / b);
return 0 ;
}

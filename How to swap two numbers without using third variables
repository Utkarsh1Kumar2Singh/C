# include <stdio.h>
int main(){
int a , b ;
printf(" Enter your first number here = ");
scanf("%d",&a);
printf(" Enter your second number here = ");
scanf(" %d",&b);
a = a + b ;
b = a - b ;
a = a - b ;
printf(" After swapping : \n");
printf(" First number = %d\n",a);
printf(" second number = %d\n",b);
return 0 ;
}
